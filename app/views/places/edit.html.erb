<!DOCTYPE html>
<html>
	<head>
		<meta charset="UTF-8">
		<title></title>
	</head>
	<body>
		<div class="booyah-box col-10 offset-1">
			<h1>Edit Your Place</h1>
			<%= simple_form_for @place do |f| %>
				<%= f.input :name %>
				<%= f.input :address %>
				<%= f.input :description %>
				<br />
				<%= f.submit 'Edit', class: 'btn btn-outline-secondary' %>
			<% end %>
		</div>
	</body>
</html>